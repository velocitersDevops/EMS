<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

          <hibernate-mapping>
          <class name="com.velociter.ems.model.Address" table="ADDRESSES">
          <id name="addressId" column="">
          <generator class="increment"></generator>
          </id>
	       <property name="addressLine1" column="ADDRESSLINE1"></property>
	       <property name="streetNumber" column="STREETNUMBER"></property>
	       <property name="cityName" column="CITY"></property>
	       <property name="stateName" column="STATE"></property>
           <property name="pincodeNumber" column="PINCODE"></property>
	       <property name="countryName" column="COUNTRY"></property>
	       <property name="addressType" column="ADDRESSTYPE"></property>
	       <property name="houseNumber" column="HOUSENUMBER"></property>
          </class>
          </hibernate-mapping>